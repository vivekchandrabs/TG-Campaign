ToSeries: function(series_id) {\r\n      this.$router.push({\r\n        name: \"Series\",\r\n        params: { series_id: series_id }\r\n      });\r\n    },\r\n    createSeries: function() {\r\n      this.$router.push({\r\n        path: \"/create-series/\"\r\n      });\r\n    }\r\n  },\r\n  created() {\r\n    if (!localStorage[\"token\"]) {\r\n      this.$router.push({\r\n        path: \"/\"\r\n      });\r\n    }\r\n\r\n    this.$store.dispatch(\"getAllSeries\").then(res => {\r\n      this.$store.commit(\"commitAllSeries\", res);\r\n    });\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.flex-container {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  background-color: #fff;\r\n}\r\n.series:hover {\r\n  border-color: coral;\r\n  box-shadow: 5px 4px 10px #ddd;\r\n  cursor: pointer;\r\n}\r\n.series {\r\n  color: black;\r\n  text-decoration: none;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components"}]}